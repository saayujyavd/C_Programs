#include<stdio.h>
int main(int argc, char* argv[], char* envp[]) {
	int rows, cols, i, j, cnt = 0;
	printf("Enter the number of rows: ");
	scanf("%d", &rows);
	printf("Enter the number of columns: ");
	scanf("%d", &cols);
	int matrix1[rows][cols], numElmnts = rows * cols, matrix2[cols][rows], mult[numElmnts];
	printf("Enter the elements into the 1st matrix:\n");
	for (i = 0; i < rows; i++) {
		for(j = 0; j < cols; j++)
			scanf("%d", &matrix1[i][j]);
	}
	printf("Enter the elements into the 2nd matrix:\n");
	for (i = 0; i < cols; i++) {
		for(j = 0; j < rows; j++)
			scanf("%d", &matrix2[i]);
	}
	for (i = 0; i < rows; i++, cnt++) {
		for (j = 0; j < cols; j++)
			mult[cnt] += matrix1[i][j] * matrix2[j][i];
	}
	printf("The multiplied matrix is:\n");
	for (i = 0; i < numElmnts; i++) {
		if (i == cols)
			printf("\n");
		printf("%d ", mult[i]);
	}

	return;
}
